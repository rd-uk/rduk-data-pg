{
  "name": "@rduk/data-pg",
  "version": "0.0.7",
  "description": "PostgreSQL Data Provider implementation for rduk-data",
  "engines": {
    "node": ">=6.4.0"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "standard --fix",
    "pretest": "npm run lint && cp ./spec/resources/config.yml ./config.yml",
    "test": "istanbul cover jasmine --report cobertura",
    "postest": "rm ./config.yml",
    "report": "istanbul report",
    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rd-uk/rduk-data-pg.git"
  },
  "bugs": {
    "url": "https://github.com/rd-uk/rduk-data-pg/issues"
  },
  "homepage": "https://github.com/rd-uk/rduk-data-pg#readme",
  "keywords": [
    "rduk",
    "data",
    "provider",
    "pg"
  ],
  "author": "RDUK <tech@rduk.fr>",
  "license": "MIT",
  "dependencies": {
    "@rduk/configuration": "2.1.1",
    "@rduk/data": "0.1.1",
    "@rduk/errors": "1.3.0"
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "istanbul": "0.4.5",
    "jasmine": "2.8.0",
    "pg": "7.4.1",
    "standard": "10.0.3"
  }
}
